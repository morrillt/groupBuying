!!!5
%html
  = render "admin/shared/head"
  %body
    .container
      .top-container
        #header
          .logo
            %a{:href=>"/admin"} Admin / Group Buying Analytics
          .account
            %a{:href=>"/admin/sites"} Sites
            %a{:href=>"/admin/deals"} Deals
            %a{:href=>"/admin/divisions"} Divisions
            %a{:href=>"/admin/snapshots"} Snapshots
            %a{:href=>"/admin/deals/export"} Deals CSV Export
      %div
        #main
          = yield
      .bottom-container
        #bottom

  = javascript_include_tag "thirdparty/jquery-1.5.1.min"
  = javascript_include_tag "thirdparty/MyAjax/ajaxTables"
  :javascript
    var model= "#{@model_name}";
    loadTable();
    label2value();

  = yield :javascripts